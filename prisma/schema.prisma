// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id          String    @id @default(uuid())
  isbn        Int
  title       String
  author      String
  description String
  price       Float
  quantity    Int

  @@map("books")
}
